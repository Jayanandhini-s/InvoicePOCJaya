@model IEnumerable<InvoicePOCJaya.Models.Invoice>


@{
    ViewData["Title"] = "Index";
    
}

<h1>Index:</h1>

<a asp-action="Create" asp-controller="Items">Add Invoice Items</a>

<table class="table">
    <thead>
        <tr>
            <th>
               Invoice Id
            </th>
            <th>
               Date
            </th>
            <th>
               Total
            </th>
           
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InvoiceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvoiceDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvoiceTotal)
            </td>
            
           
        </tr>
}
    </tbody>
</table>
