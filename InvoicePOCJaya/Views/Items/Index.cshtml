@model IEnumerable<InvoicePOCJaya.Models.Item>


@{
    ViewData["Title"] = "Index";
    
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
               Invoice Id 
            </th>
            <th>
               Item Name 
            </th>
            <th>
                Quantity 
            </th>
           <th>
                Unit Price 
            </th>
            <th>
                Line Total 
            </th>
            <th>

            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Invoice.InvoiceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UnitPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LineTotal)
            </td>
           
        </tr>
}
    </tbody>
</table>

